{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "Trustcar API",
    "description": "API for Trustcar used car inspection app"
  },
  "host": "localhost:8080",
  "basePath": "/api",
  "schemes": [
    "http"
  ],
  "securityDefinitions": {
    "Bearer": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header",
      "description": "Enter your bearer token in the format [Bearer yourToken]"
    }
  },
  "security": [
    {
      "Bearer": []
    }
  ],
  "paths": {
    "/v1/user/login": {
      "$ref": "paths/user/login.json"
    },
    "/v1/user/register": {
      "$ref": "paths/user/register.json"
    },
    "/v1/report/get": {
      "$ref": "paths/report/get.json"
    },
    "/v1/report/save": {
      "$ref": "paths/report/save.json"
    },
    "/v1/report/structure/get": {
      "$ref": "paths/report/structure/get.json"
    }
  }
}
