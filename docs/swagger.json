{
  "swagger": "2.0",
  "info": {
    "version": "1.0.0",
    "title": "Trustcar API",
    "description": "API for Trustcar used car inspection app"
  },
  "host": "localhost:8080",
  "basePath": "/api",
  "schemes": [
    "http"
  ],
  "securityDefinitions": {
    "Bearer": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header",
      "description": "Enter your bearer token in the format [Bearer YOUR_AUTH_TOKEN]"
    }
  },
  "security": [
    {
      "Bearer": []
    }
  ],
  "paths": {
    "/v1/user/login": {
      "$ref": "paths/user/login.json"
    },
    "/v1/user/register": {
      "$ref": "paths/user/register.json"
    },
    "/v1/report": {
      "$ref": "paths/report/index.json"
    },
    "/v1/report/structure": {
      "$ref": "paths/report/structure/index.json"
    }
  }
}
